<div class="row" style="background-color:white; positon:inherit;">
  <div class="col s2">
  </div>
  
  <div id="col s8 center">
    <%= form_with(model: store, local: true) do |form| %>
  <% if store.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(store.errors.count, "error") %> prohibited this store from being saved:</h2>

      <ul>
      <% store.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :street %>
    <%= form.text_field :street %>
  </div>

  <div class="field">
    <%= form.label :city %>
    <%= form.text_field :city %>
  </div>

  <div class="field">
    <%= form.label :state %>
    <%= form.text_field :state %>
  </div>

  <div class="field">
    <%= form.label :zip %>
    <%= form.text_field :zip %>
  </div>

  <div class="field">
    <%= form.label :phone %>
    <%= form.text_field :phone %>
  </div>

  <div class="field">
    <%= form.label :latitude %>
    <%= form.text_field :latitude %>
  </div>

  <div class="field">
    <%= form.label :longitude %>
    <%= form.text_field :longitude %>
  </div>

  <div class="field">
    <p>
  <label>
    <%= form.check_box :active, id:"Active" %>
    <span><%= form.label :active %></span>
  </label>
    </p>
  </div>

  <br>

  <div class="actions">
    <%= button_to "Create Store", form.submit, :style => "background-color: #d147a3 ;color: white; font-size: 16px;",
 :method => :get %>
  </div>
<% end %>
</div>
  
  <div class="col s2">
  </div>
</div>